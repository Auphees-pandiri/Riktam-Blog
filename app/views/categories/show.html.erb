<h1 align="center"><%= "Category Name: " + @category.name %></h1>
<div align="center">
	<% if logged_in? && current_user.admin? %>
	    <div class="panel-footer">
	    	<%= link_to 'Edit this category', edit_category_path(@category), class:'btn btn-primary' %> 
	    </div>
  	<% end %>
	<%= will_paginate @category_articles %>
</div>
<%= render 'articles/article', obj: @category_articles %>
<div align="center">
	<%= will_paginate @category_articles %>
</div>